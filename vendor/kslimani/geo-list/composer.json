{
    "name": "kslimani/geo-list",
    "description": "PHP only country-list currency-list and language-list.",
    "keywords": ["country", "currency", "geo", "language"],
    "license": "MIT",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "umpirsky/country-list": "^2.0",
        "umpirsky/currency-list": "^1.1",
        "umpirsky/language-list": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Sk\\Geo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "build": "php bin/build.php",
        "test": "phpunit"
    },
    "prefer-stable": true
}
