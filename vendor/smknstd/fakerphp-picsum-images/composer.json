{
    "name": "smknstd/fakerphp-picsum-images",
    "description": "Alternative image provider for fakerphp using picsum.photos ",
    "keywords": [
        "faker",
        "php",
        "images",
        "picsum"
    ],
    "homepage": "https://github.com/smknstd/fakerphp-picsum-images",
    "license": "MIT",
    "authors": [
        {
            "name": "Arnaud Becher",
            "email": "arnaud.becher@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "fakerphp/faker": "^1.10"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.28",
        "vimeo/psalm": "^4.8"
    },
    "suggest": {
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Smknstd\\FakerPicsumImages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smknstd\\FakerPicsumImages\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
